{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "FlagsmithConfiguration": {
    "EnvironmentKey": "<ENVIRONMENT_KEY>",
    "UseLegacyIdentities": true,
    "EnableClientSideEvaluation": false,
    "EnvironmentRefreshIntervalSeconds": 60,
    "EnableAnalytics": true,
    "RequestTimeout": 10,
    "Retries": 3
  },
  "AllowedHosts": "*"
}
